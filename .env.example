# Common
VITE_DATA_PROVIDER=api
VITE_ENABLE_REQUEST_WORKFLOW=true
VITE_ENABLE_DEV_LOGIN=true

# Local development
VITE_API_BASE_URL=http://localhost:5000
VITE_AUTH_AUTHORITY=https://login.microsoftonline.com/<tenant-id>/oauth2/v2.0/authorize
VITE_AUTH_CLIENT_ID=<client-id>
VITE_AUTH_TENANT_ID=<tenant-id>
VITE_AUTH_REDIRECT_URI=http://localhost:3000
VITE_AUTH_AUDIENCE=api://<api-client-id>
VITE_AUTH_SCOPE=api://<api-client-id>/access_as_user openid profile email
VITE_APP_BASE=/

